<%= semantic_form_for @dentist do |f| %>
  <%= f.inputs do %>
    <%= f.input :first %>
    <%= f.input :last %>
    <%= f.input :email %>
    <%= f.input :title %>
    <%= f.input :address1 %>
    <%= f.input :address2 %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
    <%= f.input :fax %>
  <% end %>
  <%= f.buttons %>
<% end %>

